{
  "version": "1.10.0-*",
  "description": "Multi-platform support library with a focus on asynchronous I/O.",
  "licenseUrl": "https://raw.githubusercontent.com/aspnet/libuv-build/775a18ca77368a0f4ca753e82b2369f24707fb3e/build/License.txt",
  "authors": [
    "Libuv"
  ],
  "packOptions": {
    "repository": {
      "type": "git",
