  "dependencies": {
    "Microsoft.NETCore.Platforms": "1.0.1",
    "Microsoft.AspNetCore.Internal.libuv-Darwin": {
      "version": "1.10.0-*",
      "type": "build"
    },
    "Microsoft.AspNetCore.Internal.libuv-Linux": {
      "version": "1.10.0-*",
      "type": "build"
    },
    "Microsoft.AspNetCore.Internal.libuv-Windows": {
      "version": "1.10.0-*",
      "type": "build"
    }
  },
  "frameworks": {
    "netstandard1.0": {}
